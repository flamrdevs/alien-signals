{
	"name": "alien-signals",
	"version": "2.0.4",
	"description": "The lightest signal library.",
	"license": "MIT",
	"keywords": [
		"signal"
	],
	"type": "module",
	"main": "./dist/esm/index.mjs",
	"types": "./dist/esm/index.d.mts",
	"exports": {
		".": {
			"require": {
				"types": "./dist/cjs/index.d.cts",
				"default": "./dist/cjs/index.cjs"
			},
			"import": {
				"types": "./dist/esm/index.d.mts",
				"default": "./dist/esm/index.mjs"
			}
		},
		"./system": {
			"require": {
				"types": "./dist/cjs/system.d.cts",
				"default": "./dist/cjs/system.cjs"
			},
			"import": {
				"types": "./dist/esm/system.d.mts",
				"default": "./dist/esm/system.mjs"
			}
		}
	},
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/johnsoncodehk/signals.git"
	},
	"scripts": {
		"prepublishOnly": "npm run build",
		"build": "tsdown",
		"pretest": "npm run build",
		"test": "vitest run",
		"lint": "tsslint --project tsconfig.json",
		"bench": "npm run build && node --jitless --expose-gc benchs/propagate.mjs",
		"bench:memory": "npm run build && node --expose-gc benchs/memoryUsage.mjs"
	},
	"devDependencies": {
		"@tsslint/cli": "latest",
		"@tsslint/config": "latest",
		"jest-extended": "latest",
		"mitata": "latest",
		"publint": "latest",
		"tsdown": "latest",
		"typescript": "latest",
		"vitest": "latest"
	},
	"packageManager": "pnpm@9.12.0"
}
